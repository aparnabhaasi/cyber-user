{
    "version": 2,
    "builds": [
      {
        "src": "src/package.json",
        "use": "@vercel/next",
        "config": {
          "distDir": "src/.next"
        }
      }
    ],
    "framework": "nextjs",
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/src/$1"
      }
    ]
  }
  